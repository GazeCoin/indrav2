{
  "name": "indra-node",
  "version": "7.0.0-alpha.3",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf ./dist/* && tsc -p tsconfig.json",
    "build-bundle": "webpack --config ops/webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "../../node_modules/.bin/eslint -c '../../.eslintrc.js' --fix 'src/**/*'",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.json\" ",
    "start:debug": "nodemon --config ops/nodemon-debug.json",
    "migration:generate": "typeorm migration:generate -d migrations -n ",
    "prestart:prod": "rm -rf dist && npm run build",
    "start:prod": "node dist/main.js",
    "test": "ts-mocha --bail --check-leaks --exit --timeout 10000 src/**/*.spec.ts",
    "watch": "ts-mocha --bail --check-leaks --watch --timeout 10000 src/**/*.spec.ts"
  },
  "dependencies": {
    "@connext/apps": "7.0.0-alpha.3",
    "@connext/cf-core": "7.0.0-alpha.3",
    "@connext/contracts": "3.2.3",
    "@connext/messaging": "7.0.0-alpha.3",
    "@connext/store": "7.0.0-alpha.3",
    "@connext/types": "7.0.0-alpha.3",
    "@connext/utils": "7.0.0-alpha.3",
    "@nestjs/common": "7.1.2",
    "@nestjs/core": "7.1.2",
    "@nestjs/microservices": "7.1.2",
    "@nestjs/platform-express": "7.1.2",
    "@nestjs/typeorm": "7.0.0",
    "@uniswap/sdk": "1.0.0-beta.4",
    "bs58check": "2.1.2",
    "class-validator": "0.12.2",
    "dotenv": "8.2.0",
    "ethers": "5.0.0-beta.190",
    "evt": "1.7.11",
    "ioredis": "4.17.3",
    "nats": "1.4.9",
    "pg": "8.2.1",
    "redlock": "4.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "ts-natsutil": "1.0.6",
    "typeorm": "0.2.24",
    "typescript-memoize": "1.0.0-alpha.3"
  },
  "devDependencies": {
    "@ethereum-waffle/chai": "3.0.0-beta.2",
    "@connext/client": "7.0.0-alpha.3",
    "@nestjs/testing": "7.1.2",
    "@openzeppelin/contracts": "3.0.1",
    "@types/chai": "4.2.11",
    "@types/express": "4.17.6",
    "@types/ioredis": "^4.16.5",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.9",
    "@types/supertest": "2.0.9",
    "@types/chai-subset": "1.3.3",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-subset": "1.6.0",
    "concurrently": "5.2.0",
    "mocha": "7.2.0",
    "nodemon": "2.0.4",
    "supertest": "4.0.2",
    "ts-mocha": "7.0.0",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.3",
    "wait-on": "5.0.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-node-externals": "1.7.2"
  }
}
